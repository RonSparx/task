{{ "section.header.css" | asset_url | stylesheet_tag }}
<header>
{% for link in linklists.main-menu.links %}
    {% if link.links != blank %}
      <a href="{{ link.url }}">{{ link.title }}</a>
      {% for childlink in link.links %}
        <a href="{{ childlink.url }}">{{ childlink.title | escape }}</a>
      {% endfor %}
    {% else %}
      <a href="{{ link.url }}">{{ link.title }}</a>
    {% endif %}
  {% endfor %}

  <a href="/cart">cart</a>
</header>